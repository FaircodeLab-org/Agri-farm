ase Invoice Item` AS c\n    INNER JOIN `tabPurchase Invoice` AS p ON c.parent = p.name\n    WHERE p.custom_survey = %s\n\"\"\", (doc.name,), as_dict=1) %}\n\n\n{% for k in s %}\n               \n\n\n\n{% for i in doc.crop %}\n\n{% if i.crop_variant==k.item_code %}\n{% set yield_qty=i.yield_qty_in_kg %}\n{% set avail_qty=i.available_qty_kg %}\n{% set sale_qty=i.total_other_sale_qty_kg %}\n        <tr style=\"line-height:110%\">\n            <td>{{ k.item_code }}</td>\n            <td>{{yield_qty}}</td>\n            <td>{{ k.qty }}</td>\n            <td>{{ k.rate }}</td>\n            <td>{{ k.amount }}</td>\n            <td>{{ k.parent }}</td>\n            <td>{{avail_qty}}</td>\n            <td>{{sale_qty}}</td>\n        </tr>\n    \n{% endif %}\n   {% endfor %} \n  {% endfor %}\n        \n    </table>\n     \n </div>\n    \n    \n\n    \n    \n    \n    \n    \n      \n    <div class=\"row\">\n     <p style=\"font-size:14px; color:black\">&#9679; <b>Labour Details</b></p>\n      \n    <table border=.1px width=\"100%\">\n        <tr>\n            \n            <th width=50%><b></b></th>\n            <th width=15%><b></b></th>\n            <th width=20%><b>Numbers</b></th>\n           \n        </tr>\n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n             \n             \n             {% for i in doc.labour_questions %}\n               {% if i.included_in_farm_diary==1%}\n         \n         <tr style=\"line-height:120%\">\n             \n              \n                <td>{{i.questions}}</td>\n                <td>Seasonal / Migrant / Permanent</td>\n                <td>{{i.remark}}</td>\n              \n                {% endif%}\n            \n            {% endfor %}\n        </tr>\n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n        \n     \n    \n\n       \n    