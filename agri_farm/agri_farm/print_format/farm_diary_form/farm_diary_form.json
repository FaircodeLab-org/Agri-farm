d>\n\n\n    </tr>\n  \n  \n   <tr>\n        <td style=\"width:25%\" >\n            <b>Pincode </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].pin_code}}\n        </td>\n        <td style=\"width:25%\" >\n            <b>Area(Ha) </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{doc.farm_area}}\n        </td>\n\n\n    </tr>\n    \n       <tr>\n            <td style=\"width:25%\">  \n           <b>Total Farm Land (Acre)</b>\n           </td>\n           <td style=\"width:25%\">  \n          : {{s[0].total_farmland}}\n           </td>\n        \n        <td style=\"width:25%\">  \n           <b>Address</b>\n           </td>\n           <td style=\"width:25%\">  \n           {{s[0].address}}\n           </td>\n\n\n    </tr>\n    \n    \n\n      <tr>\n        <td style=\"width:25%\" >\n            <b>National ID Card </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].id_card}}\n        </td>\n          <td style=\"width:25%\" >\n            <b>ID Card No</b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].id_card_number}}\n        </td>\n\n\n    </tr>\n    \n    \n     <tr>\n        \n        \n         <td style=\"width:25%\" >\n            <b>Date Of Birth </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].date_of_birth}}\n        </td>\n        <td style=\"width:25%\">  \n           <b>Families Member</b>\n           </td>\n           <td style=\"width:25%\">  \n          : {{s[0].families_members}}\n           </td>\n\n    </tr>\n    \n    \n    \n    \n    \n    \n    \n    \n      <tr>\n        <td style=\"width:25%\" >\n            <b>Field Inspector Name</b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{doc.field_staff_name}}\n        </td>\n        <td style=\"width:25%\">  \n           <b>Bank Name</b>\n           </td>\n           <td style=\"width:25%\">  \n          : {{s[0].bank_name}}\n           </td>\n\n\n    </tr>\n    \n      \n    \n      <tr>\n        <td style=\"width:25%\" >\n            <b>A/C Holder Name </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].ac_holder_name}}\n        </td>\n        <td style=\"width:25%\">  \n           <b>Bank A/C Number</b>\n           </td>\n           <td style=\"width:25%\">  \n          : {{s[0].bank_acc_no}}\n           </td>\n\n\n    </tr>\n    \n    \n    \n     <tr>\n        <td style=\"width:25%\" >\n            <b>Bank Code </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].bank_ifsc}}\n        </td>\n        <td style=\"width:25%\">  \n           <b>Bank Branch</b>\n           </td>\n           <td style=\"width:25%\">  \n          : {{s[0].bank_branch}}\n           </td>\n\n\n    </tr>\n    \n      <tr>\n        <td style=\"width:25%\" >\n            <b>Member Since </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].member_since}}\n        </td>\n        <td style=\"width:25%\">  \n           <b>Email ID</b>\n           </td>\n           <td style=\"width:25%\">  \n          : {{s[0].email_id}}\n           </td>\n\n\n    </tr>\n    \n    \n      <tr>\n       \n        <td style=\"width:25%\">  \n           <b>Father Name</b>\n           </td>\n           <td style=\"width:25%\">  \n          : {{s[0].father_name}}\n           </td>\n              <td style=\"width:25%\" >\n            <b>Season </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].season}}\n        </td>\n\n\n    </tr>\n    \n      <tr>\n        <td style=\"width:25%\" >\n            <b>Project </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].project}}\n        </td>\n    \n        \n     <td style=\"width:25%\" >\n            <b>Status </b> \n        </td>\n        <td style=\"width:25%\" >\n           : {{s[0].status}}\n        </td>\n      \n        \n        \n        \n     \n\n    </tr>\n    \n    <!--  <tr>-->\n    <!--